{ "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "markdown": { "idInHeadings": true },
  "templates": { "cleverLinks": true },
  "opts": {
    "destination": "./doc/",
    "readme": "./README.md",
    "recurse": true
  }
}
